{
  "version": 2,
  "routes": [
    {
      "src": "/(index.html|Pages/.*|Styles/.*|components/.*)",
      "dest": "$0"
    }
  ],
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "Pages/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "Styles/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "components/*.html",
      "use": "@vercel/static"
    }
  ]
}